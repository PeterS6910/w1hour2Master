<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="Contal.Cgp.Server.Beans.TimeZoneDateSetting, Cgp.Server.Beans" select-before-update="true">
    <cache usage="read-write"/>
    <id name="IdTimeZoneDateSetting">
      <generator class="guid" />
    </id>
    <property name="SetYear" not-null="false" unique-key="TimeZoneSet"></property>
    <property name="Year" not-null="false" unique-key="TimeZoneSet"></property>
    <property name="Month" not-null="false" unique-key="TimeZoneSet"></property>
    <property name="Week" not-null="false" unique-key="TimeZoneSet"></property>
    <property name="Day" not-null="false" unique-key="TimeZoneSet"></property>
    <property name="ExplicitDailyPlan" not-null="false"></property>

    <many-to-one name="TimeZone" column="TimeZone" class="Contal.Cgp.Server.Beans.TimeZone, Cgp.Server.Beans" not-null="false" unique-key="TimeZoneSet"/>
    <many-to-one name="DailyPlan" column="DailyPlan" class="Contal.Cgp.Server.Beans.DailyPlan, Cgp.Server.Beans" not-null="true" unique-key="TimeZoneSet"/>
    <many-to-one name="DayType" column="DayType" class="Contal.Cgp.Server.Beans.DayType, Cgp.Server.Beans" not-null="false" unique-key="TimeZoneSet"/>
    <property name="Description" not-null="false">
      <column name="Description" sql-type="NVARCHAR(MAX)"></column>
    </property>

    <property name="Version" not-null="false"></property>
  </class>
</hibernate-mapping>
