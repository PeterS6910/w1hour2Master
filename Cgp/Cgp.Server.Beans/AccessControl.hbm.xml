<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="Contal.Cgp.Server.Beans.AccessControl, Cgp.Server.Beans" select-before-update="true">
    <cache usage="read-write"/>
    <id name="IdAccessControl">
      <generator class="guid" />
    </id>

    <many-to-one name="Login" column="Login" class="Contal.Cgp.Server.Beans.Login, Cgp.Server.Beans" not-null="false"/>
    <many-to-one name="LoginGroup" column="LoginGroup" class="Contal.Cgp.Server.Beans.LoginGroup, Cgp.Server.Beans" not-null="false"/>
    <property name="Source" not-null="false"></property>
    <property name="Access" not-null="true"></property>
  </class>
</hibernate-mapping>