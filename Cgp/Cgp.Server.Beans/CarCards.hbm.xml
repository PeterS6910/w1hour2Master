<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="Contal.Cgp.Server.Beans.CarCard, Cgp.Server.Beans" select-before-update="true">
    <cache usage="read-write"/>
    <id name="IdCarCard">
      <generator class="guid" />
    </id>
    <many-to-one name="Car" column="IdCar" class="Contal.Cgp.Server.Beans.Car, Cgp.Server.Beans" not-null="true"/>
    <many-to-one name="Card" column="IdCard" class="Contal.Cgp.Server.Beans.Card, Cgp.Server.Beans" not-null="true"/>
  </class>
</hibernate-mapping>

