<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="Contal.Cgp.Server.Beans.CalendarDateSetting, Cgp.Server.Beans" select-before-update="true">
    <cache usage="read-write"/>
    <id name="IdCalendarDateSetting">
      <generator class="guid" />
    </id>
    <property name="SetYear" not-null="false" unique-key="CalendarSet"></property>
    <property name="Year" not-null="true" unique-key="CalendarSet"></property>
    <property name="Month" not-null="true" unique-key="CalendarSet"></property>
    <property name="Week" not-null="true" unique-key="CalendarSet"></property>
    <property name="Day" not-null="true" unique-key="CalendarSet"></property>
    <many-to-one name="Calendar" column="Calendar" class="Contal.Cgp.Server.Beans.Calendar, Cgp.Server.Beans" not-null="false" unique-key="CalendarSet"/>
    <many-to-one name="DayType" column="DayType" class="Contal.Cgp.Server.Beans.DayType, Cgp.Server.Beans" not-null="true" unique-key="CalendarSet"/>
    <property name="Description" not-null="false">
      <column name="Description" sql-type="NVARCHAR(MAX)"></column>
    </property>
    <property name="Version" not-null="false"></property>
  </class>
</hibernate-mapping>
