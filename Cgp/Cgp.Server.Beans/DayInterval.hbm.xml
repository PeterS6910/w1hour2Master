<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="Contal.Cgp.Server.Beans.DayInterval, Cgp.Server.Beans" select-before-update="true">
    <cache usage="read-write"/>
    <id name="IdInterval">
      <generator class="guid" />
    </id>
    <property name="MinutesFrom" not-null="true"></property>
    <property name="MinutesTo" not-null="true"></property>
    <many-to-one name="DailyPlan" column="IdDailyPlan" class="Contal.Cgp.Server.Beans.DailyPlan, Cgp.Server.Beans" not-null="false"/>
  </class>
</hibernate-mapping>
