<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="Contal.Cgp.Server.Beans.EventlogParameter, Cgp.Server.Beans" select-before-update="true">
    <!--id is used because combination of name and surname does not have to be unique-->
    <id name="IdEventlogParameter">
      <generator class="guid" />
    </id>
    <property name="Type" not-null="true"></property>
    <property name="Value" not-null="true"></property>

    <many-to-one name="Eventlog" column="IdEventlog" class="Contal.Cgp.Server.Beans.Eventlog, Cgp.Server.Beans"/>
  </class>
</hibernate-mapping>


