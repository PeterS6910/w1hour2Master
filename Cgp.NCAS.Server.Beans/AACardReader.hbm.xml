<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="Contal.Cgp.NCAS.Server.Beans.AACardReader, Cgp.NCAS.Server.Beans" select-before-update="true">
    <cache usage="read-write"/>
    <id name="IdAACardReader">
      <generator class="guid" />
    </id>

    <many-to-one name="AlarmArea" column="AlarmArea" class="Contal.Cgp.NCAS.Server.Beans.AlarmArea, Cgp.NCAS.Server.Beans" not-null="false"/>
    <many-to-one name="CardReader" column="CardReader" class="Contal.Cgp.NCAS.Server.Beans.CardReader, Cgp.NCAS.Server.Beans" not-null="false"/>
    <property name="EnableEventlog" not-null="false"></property>
    <property name="AASet" not-null="true"></property>
    <property name="AAUnset" not-null="true"></property>
    <property name="AAUnconditionalSet" not-null="false"></property>
    <property name="PermanentlyUnlock" not-null="true"></property>
    <property name="Version" not-null="false"></property>
  </class>
</hibernate-mapping>
