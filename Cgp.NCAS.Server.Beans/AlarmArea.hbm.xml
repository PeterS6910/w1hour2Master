    <property name="AllowSendingStateToCRs" not-null="false"></property>
    
    <property name="UseEIS" not-null="false"></property>
    <property name="FilterTimeEIS" not-null="false"></property>
    <property name="ActivationStateInputEIS" not-null="false"></property>
    <property name="SetUnsetOutputEIS" not-null="false"></property>
    
    <property name="LocalAlarmInstruction" not-null="false">
      <column name="LocalAlarmInstruction" sql-type="NVARCHAR(MAX)"></column>
    </property>

    <property name="AutomaticActivationMode" not-null="false"></property>
    <property name="AlarmAreaSetByOnOffObjectFailed" not-null="false"></property>
    <property name="BlockAlarmAreaSetByOnOffObjectFailed" not-null="false"></property>
    <property name="ObjBlockAlarmAreaSetByOnOffObjectFailedObjectType" not-null="false"></property>
    <property name="ObjBlockAlarmAreaSetByOnOffObjectFailedId" not-null="false"></property>
    <property name="AlwaysProvideUnsetForTimeBuying" not-null="false"></property>
    <many-to-one name="AlarmAreaSetByOnOffObjectFailedPresentationGroup" class="Contal.Cgp.Server.Beans.PresentationGroup, Cgp.Server.Beans" not-null="false"/>

    <many-to-one name="SensorAlarmPresentationGroup" class="Contal.Cgp.Server.Beans.PresentationGroup, Cgp.Server.Beans" not-null="false"/>
    <many-to-one name="SensorTamperAlarmPresentationGroup" class="Contal.Cgp.Server.Beans.PresentationGroup, Cgp.Server.Beans" not-null="false"/>
    
    <bag name="AlarmAreaAlarmArcs" cascade="all-delete-orphan">
      <key column="AlarmArea" />
      <one-to-many class="Contal.Cgp.NCAS.Server.Beans.AlarmAreaAlarmArc"  />
    </bag>

    <property name="Version" not-null="false"></property>

    <join table="CentralNameRegister">
      <key column="Id" foreign-key="none"/>
      <property name="Name" column="Name" not-null="true"/>
      <property name="ShortName" column="AlternateName"/>
      <property name="FullTextSearchString" column="FullTextSearchString" not-null="false"/>
      <property name="ObjectType" not-null="true"/>
    </join>  
  </class>
</hibernate-mapping>
