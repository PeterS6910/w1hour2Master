<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="Contal.Cgp.NCAS.Server.Beans.PersonAttributeOutput, Cgp.NCAS.Server.Beans" select-before-update="true">
    <cache usage="read-write"/>
    <id name="Id">
      <generator class="guid" />
    </id>
    <property name="IsEnabled" not-null="false"></property>
    <property name="Interval" not-null="false"></property>
    <property name="FailsCount" not-null="false"></property>
    <property name="Output" not-null="false">
      <column name="Output" sql-type="NVARCHAR(MAX)"></column>
    </property>
    <property name="LastReportDate" not-null="false"></property>
    <property name="IdTimeZone" not-null="false">
      <column name="IdTimeZone" sql-type="uniqueidentifier"></column>
    </property>
    <!--<many-to-one name="TimeZone" column="IdTimeZone"  unique="true"  cascade="all" class="Contal.Cgp.Server.Beans.TimeZone" />-->
  </class>
</hibernate-mapping>
